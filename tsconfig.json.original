{
	"compilerOptions": {
		"baseUrl": "./",
		"module": "commonjs",
		"target": "es6",
		"outDir": "dist",
		"rootDir": ".",
		"sourceMap": true,
		"strict": true,
		"paths": {
			"@globals/*": [
				"./globals/*"
			],
			"@utilities/*": [
				"./utilities/*"
			],
			"@typedefs/*": [
				"./typedefs/*"
			]
		}
	},
	"exclude": [
		"frontend",
		"globals",
		"node_modules",
		"dist",
		"cypress"
	]
}