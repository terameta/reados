{
  "name": "reados",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "func start",
    "deploy": "func azure functionapp publish readosfu --build remote",
    "test": "echo \"No tests yet...\"",
    "ng": "ng",
    "start:fe": "cd frontend; ng serve",
    "build:fe": "cd frontend; ng build",
    "watch:fe": "ng build --watch --configuration development",
    "test:fe": "ng test",
    "test:cypress": "cypress run",
    "open:cypress": "cypress open",
    "dev": "concurrently --kill-others \"npm run start:fe\" \"npm run watch\" \"npm run start\""
  },
